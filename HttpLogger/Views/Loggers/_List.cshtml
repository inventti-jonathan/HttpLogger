@model IEnumerable<Logger>

<table class="table">
    <thead>
        <tr>
            <th>@Html.DisplayNameFor(model => model.Name)</th>
            <th></th>
        </tr>
    </thead>

    <tbody>

        @foreach (var item in Model)
        {
            <tr>
                <td><a asp-action="View" asp-route-id="@item.Id">/log/@Html.DisplayFor(model => item.Name)</a></td>
                <td>
                    <a asp-action="Clear" asp-route-id="@item.Id">Clear</a> |
                    <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
                </td>
            </tr>
        }

    </tbody>

</table>